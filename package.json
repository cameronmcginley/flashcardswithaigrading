{
  "name": "ez-anki",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "db:start": "npx supabase start --debug --ignore-health-check",
    "db:stop": "npx supabase stop",
    "db:status": "npx supabase status",
    "db:new": "npx supabase migration new",
    "db:push": "npx supabase db push",
    "db:reset": "npx supabase stop && (docker volume rm $(docker volume ls -q --filter label=com.supabase.cli.project=ez-anki) || true) && npx supabase start",
    "db:types": "npx supabase gen types typescript --local > src/types/supabase.ts",
    "db:seed": "npx supabase db reset && sleep 2 && psql -h localhost -p 54322 -U postgres -d postgres -f supabase/seed/00_auth.sql && sleep 1 && psql -h localhost -p 54322 -U postgres -d postgres -f supabase/seed/01_categories.sql && sleep 1 && psql -h localhost -p 54322 -U postgres -d postgres -f supabase/seed/02_decks.sql && sleep 1 && psql -h localhost -p 54322 -U postgres -d postgres -f supabase/seed/03_cards.sql && sleep 1 && psql -h localhost -p 54322 -U postgres -d postgres -f supabase/seed/04_review_logs.sql",
    "db:hardreset": "npm run db:reset && npm run db:seed"
  },
  "dependencies": {
    "@nivo/bar": "^0.94.0",
    "@nivo/calendar": "^0.94.0",
